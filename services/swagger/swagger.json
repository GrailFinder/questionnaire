{
    "swagger": "2.0",
    "info": {
      "version": "0.0.1",
      "title": "questmaker",
      "description": "Swagger spec for documenting the questmaker"
    },
    "host": "192.168.99.100",
    "schemes": [
      "http"
    ],
    "securityDefinitions": {
      "Bearer": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    },
    "paths": {
      "/ping": {
        "get": {
          "description": "Returns a sanity check",
          "produces": [
            "application/json"
          ],
          "responses": {
            "200": {
              "description": "Will return 'pong!'"
            }
          }
        }
      },
      "/inquiries": {
        "get": {
          "description": "Returns all inquiries",
          "produces": [
            "application/json"
          ],
          "responses": {
            "200": {
              "description": "inquiry object"
            }
          }
        }
      },
    "definitions": {
      "User": {
        "type": "object",
        "required": [
          "username",
          "email",
          "password"
        ],
        "properties": {
          "username": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      }
    }
}
}
  